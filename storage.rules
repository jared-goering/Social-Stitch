rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /posts/{allPaths=**} {
      allow read: if true;
      allow write: if true;
    }
    match /styles/{allPaths=**} {
      allow read: if true;
      allow write: if true;
    }
    match /mockups/{userId}/{allPaths=**} {
      allow read: if true;
      allow write: if true;
    }
    match /scheduled/{allPaths=**} {
      allow read: if true;
      allow write: if true;
    }
  }
}
